{
	"name": "biome",
	"version": "0.0.1",
	"private": true,
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"nxst": "./src/index.ts",
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		}
	},
	"nx": {
		"targets": {
			"build": {
				"executor": "@nx/js:tsc",
				"outputs": [
					"{options.outputPath}"
				],
				"options": {
					"outputPath": "tools/biome/dist",
					"main": "tools/biome/src/index.ts",
					"tsConfig": "tools/biome/tsconfig.lib.json",
					"rootDir": "tools/biome/src",
					"generatePackageJson": false,
					"assets": [
						{
							"input": "./tools/biome/src",
							"glob": "**/!(*.ts)",
							"output": "."
						},
						{
							"input": "./tools/biome/src",
							"glob": "**/*.d.ts",
							"output": "."
						}
					]
				}
			}
		}
	},
	"dependencies": {
		"tslib": "^2.3.0"
	}
}
