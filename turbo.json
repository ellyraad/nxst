{
	"$schema": "https://turborepo.com/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*", "!**/*.stories.{tsx,jsx,mdx}"],
			"outputs": [".next/**", "!.next/cache/**", "storybook-static/**"]
		},
		"build:storybook": {},
		"//#format-and-lint": {},
		"//#format-and-lint:fix": {
			"cache": false
		},
		"check-types": {
			"dependsOn": ["^check-types"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
